{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "curly": "error",
    "eqeqeq": "error",
    "indent": ["error", 2],
    "no-console": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
